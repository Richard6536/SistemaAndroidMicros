<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.micros.sistemaandroidmicros.HistorialIdaVueltaFragment">
    <!-- TODO: Update blank fragment layout -->
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_width="270dp"
                android:background="#a9fcab"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <TextView
                    android:id="@+id/txtPasajerosTransportados"
                    android:layout_width="227dp"
                    android:layout_height="50dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="18dp"
                    android:layout_marginStart="18dp"
                    android:layout_marginTop="27dp"
                    android:text="Pasajeros transportados que usan STAPP"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtPasajerosTransp"
                    android:layout_width="227dp"
                    android:layout_height="60dp"
                    android:text="Text"
                    android:layout_below="@+id/txtPasajerosTransportados"
                    android:layout_alignLeft="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_alignStart="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_marginTop="7dp" />

                <TextView
                    android:id="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_width="227dp"
                    android:layout_height="84dp"
                    android:text="Hora de Inicio: "
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:layout_below="@+id/txtPasajerosTransp"
                    android:layout_alignLeft="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignStart="@+id/txtHoraTerminoIdaVuelta" />


                <TextView
                    android:id="@+id/txtHoraInicioIdaVuelta"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:text="Text"
                    android:layout_alignLeft="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_alignStart="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_alignRight="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_alignEnd="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_above="@+id/txtHoraTerminoIdaVuelta" />

                <TextView
                    android:id="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_width="227dp"
                    android:layout_height="20dp"
                    android:layout_alignLeft="@+id/txtPasajerosTransportados"
                    android:layout_alignStart="@+id/txtPasajerosTransportados"
                    android:layout_below="@+id/txtvHoraInicioIdaVuelta"
                    android:text="Hora de Término"
                    android:textSize="18dp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:text="TextView"
                    android:layout_alignLeft="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignStart="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignRight="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignEnd="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_below="@+id/txtHoraTerminoIdaVuelta" />

                <TextView
                    android:id="@+id/txtDuracionRec"
                    android:layout_width="227dp"
                    android:layout_height="84dp"
                    android:text="Duración del Recorrido"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:layout_below="@+id/textView16"
                    android:layout_alignLeft="@+id/textView16"
                    android:layout_alignStart="@+id/textView16" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_alignLeft="@+id/txtDuracionRec"
                    android:layout_alignStart="@+id/txtDuracionRec"
                    android:layout_below="@+id/textView16"
                    android:layout_marginTop="24dp"
                    android:text="TextView"
                    android:layout_alignRight="@+id/txtDuracionRec"
                    android:layout_alignEnd="@+id/txtDuracionRec" />

                <LinearLayout
                    android:id="@+id/fullscreen_content_controls"
                    style="?metaButtonBarStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center_horizontal"
                    android:background="#258928"
                    android:orientation="horizontal"
                    tools:ignore="UselessParent"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                    <Button
                        android:id="@+id/btnHistorialParadero"
                        style="?metaButtonBarButtonStyle"
                        android:layout_width="0dp"
                        android:background="@android:color/transparent"
                        android:textColor="#ffff"
                        android:clickable="true"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Historial Paradero" />

                </LinearLayout>

            </RelativeLayout>
        </RelativeLayout>

    </FrameLayout>


</RelativeLayout>
