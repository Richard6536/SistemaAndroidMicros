<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.micros.sistemaandroidmicros.HistorialParaderoFragment">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_width="270dp"
                android:background="#a9fcab"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <TextView
                    android:id="@+id/txtPasajerosTransportados"
                    android:layout_width="227dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="18dp"
                    android:layout_marginStart="18dp"
                    android:layout_marginTop="27dp"
                    android:text="Hora de Llegada"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtHoraLlegadaParaderos"
                    android:layout_width="227dp"
                    android:layout_height="60dp"
                    android:text="Text"
                    android:layout_marginTop="7dp"
                    android:layout_below="@+id/txtPasajerosTransportados"
                    android:layout_alignLeft="@+id/txtPasajerosTransportados"
                    android:layout_alignStart="@+id/txtPasajerosTransportados" />

                <TextView
                    android:id="@+id/txtvHoraInicioIdaVuelta"
                    android:layout_width="227dp"
                    android:layout_height="84dp"
                    android:text="Tiempo Detenido"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:layout_below="@+id/txtHoraLlegadaParaderos"
                    android:layout_alignLeft="@+id/txtHoraLlegadaParaderos"
                    android:layout_alignStart="@+id/txtHoraLlegadaParaderos" />


                <TextView
                    android:id="@+id/txtTiempoDetenido"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="137dp"
                    android:text="Text"
                    android:layout_above="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignLeft="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignStart="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignRight="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignEnd="@+id/txtHoraTerminoIdaVuelta" />

                <TextView
                    android:id="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_width="227dp"
                    android:layout_height="20dp"
                    android:layout_alignLeft="@+id/txtPasajerosTransportados"
                    android:layout_alignStart="@+id/txtPasajerosTransportados"
                    android:layout_below="@+id/txtvHoraInicioIdaVuelta"
                    android:text="Pasajeros Recibidos"
                    android:textSize="18dp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/txtPasajerosRecibidos"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:text="TextView"
                    android:layout_centerVertical="true"
                    android:layout_alignLeft="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignStart="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignRight="@+id/txtHoraTerminoIdaVuelta"
                    android:layout_alignEnd="@+id/txtHoraTerminoIdaVuelta" />

            </RelativeLayout>
        </RelativeLayout>

    </FrameLayout>


</RelativeLayout>
